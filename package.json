{
  "name": "@human.tech/passport-embed",
  "version": "0.3.0",
  "description": "Human Passport Embeddable Widgets",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/passportxyz/passport-embed.git"
  },
  "license": "AGPL-3.0-or-later",
  "author": "human.tech",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "private": false,
  "sideEffects": false,
  "browserslist": [
    ">0.2%",
    "not dead",
    "last 2 versions",
    "not IE 11"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "setup": "[ ! -f dev/.env ] && cp dev/.env.mock dev/.env || true; yarn install && cd dev && yarn install",
    "clean": "rm -rf dist node_modules dev/node_modules dev/dist",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "cd dev && npx playwright test",
    "test:e2e:ui": "cd dev && npx playwright test --ui",
    "test:e2e:debug": "cd dev && npx playwright test --debug",
    "test:e2e:headed": "cd dev && npx playwright test --headed",
    "test:all": "yarn test && yarn test:e2e",
    "build": "yarn build:bundle && yarn build:types",
    "build:all": "yarn build && cd dev && npx tsc && npx vite build",
    "localpub": "yarn build && yalc push",
    "build:bundle": "webpack",
    "build:types": "tsc --declaration --declarationDir dist --emitDeclarationOnly",
    "dev": "cd dev && npx vite",
    "dev:mock": "cd dev && VITE_ENABLE_MSW=true npx vite",
    "dev:real": "cd dev && VITE_ENABLE_MSW=false npx vite",
    "dev:setup": "yarn install && yarn build && cd dev && yarn install",
    "watch:css": "postcss 'src/**/*.css' -d dist/esm/ --base src --watch",
    "watch:esm": "tsc --module ESNext --outDir dist/esm --watch",
    "lint": "eslint .",
    "prettier": "prettier --write ."
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/runtime": "^7.26.7",
    "@babel/runtime-corejs3": "^7.26.7",
    "@eslint/js": "^9.19.0",
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.49.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "core-js": "3",
    "css-loader": "^7.1.2",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.7.0",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.11",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.23.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "yalc": "^1.0.0-pre.53"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "@holonym-foundation/human-id-sdk": "^0.0.17",
    "@tanstack/react-query": "^5.62.11",
    "axios": "^1.7.7",
    "dompurify": "^3.2.4",
    "html-react-parser": "^5.2.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
